{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/icons/**",
          "/*.(svg|png|jpg|jpeg|gif|webp)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "firebase-api",
      "urls": [
        "https://firestore.googleapis.com/**",
        "https://securetoken.googleapis.com/**",
        "https://www.googleapis.com/**",
        "https://cigfree-6acdb.firebaseapp.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "5s"
      }
    },
    {
      "name": "connectivity-test",
      "urls": [
        "/assets/connectivity-test.json"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "0u",
        "timeout": "2s"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ]
}